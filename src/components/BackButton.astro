<button type="button" id="back-button">Go Back</button>

<script>
	const button = document
		.getElementById("back-button")
		?.addEventListener("click", () => {
			history.back();
		});
	console.log(button);
</script>
<style>
	button {
		color: var(--aliceBlue);
		background-color: var(--gunmetal);
		position: absolute;
		left: 0;
		margin-left: 10px;
		padding: 15px;
		border-radius: 50px;
		border: none;
	}
	button:hover {
		background-color: var(--roseQuartz);
	}
	button:active {
		background-color: black;
	}
</style>
